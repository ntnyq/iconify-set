{
  "name": "iconify-set",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.14.2",
  "description": "Check if given string is an iconify icon set name",
  "keywords": [
    "iconify",
    "iconify-set",
    "iconify-set-name",
    "iconify-collection",
    "validate",
    "check"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/iconify-set#readme",
  "repository": "ntnyq/iconify-set",
  "bugs": {
    "url": "https://github.com/ntnyq/iconify-set/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "iconify:pretty": "prettier --write src/iconify.json",
    "iconify:update": "tsx scripts/update.ts",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit",
    "update": "run-s iconify:update iconify:pretty"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.3.0",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.9.1",
    "@vitest/coverage-v8": "^2.1.5",
    "bumpp": "^9.8.1",
    "consola": "^3.2.3",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "vitest": "^2.1.5"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "overrides": {
      "@rollup/plugin-commonjs": "^28.0.1"
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
