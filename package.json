{
  "name": "iconify-set",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@10.14.0",
  "description": "Check if given string is an iconify icon set name",
  "keywords": [
    "check",
    "iconify",
    "iconify-collection",
    "iconify-set",
    "iconify-set-name",
    "validate"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/iconify-set#readme",
  "repository": "ntnyq/iconify-set",
  "bugs": {
    "url": "https://github.com/ntnyq/iconify-set/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch src",
    "iconify:pretty": "prettier --write src/iconify.json",
    "iconify:update": "tsx scripts/update.ts",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit",
    "update": "run-s iconify:update iconify:pretty"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.3.1",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.3.0",
    "bumpp": "^10.2.3",
    "consola": "^3.4.2",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,json}": "eslint --fix"
  }
}
