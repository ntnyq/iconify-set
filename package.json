{
  "name": "iconify-set",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.10.0",
  "description": "Check if given string is an iconify icon set name",
  "keywords": [
    "check",
    "iconify",
    "iconify-collection",
    "iconify-set",
    "iconify-set-name",
    "validate"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/iconify-set#readme",
  "repository": "ntnyq/iconify-set",
  "bugs": {
    "url": "https://github.com/ntnyq/iconify-set/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch src",
    "iconify:pretty": "prettier --write src/iconify.json",
    "iconify:update": "tsx scripts/update.ts",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit",
    "update": "run-s iconify:update iconify:pretty"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.2.0",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "consola": "^3.4.2",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.1",
    "prettier": "^3.5.3",
    "tsdown": "^0.11.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
